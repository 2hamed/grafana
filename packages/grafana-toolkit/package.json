{
  "name": "@grafana/toolkit",
  "version": "6.0.1-alpha.0",
  "description": "Grafana Runtime Library",
  "keywords": [
    "typescript",
    "react",
    "react-component"
  ],
  "bin": {
    "grafana-toolkit": "./bin/grafana-toolkit.js"
  },
  "scripts": {
    "tslint": "tslint -c tslint.json --project tsconfig.json",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf ./dist ./compiled",
    "build": "rollup -c rollup.config.ts",
    "cli": "ts-node --project ./cli/tsconfig.json ./cli/index.ts",
    "themes:generate": "ts-node --project ./tsconfig.json ./src/cli/generateSassVariableFiles.ts"
  },
  "author": "Grafana Labs",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/commander": "^2.12.2",
    "ts-node": "^8.2.0"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/commander": "^2.12.2",
    "@types/execa": "^0.9.0",
    "@types/inquirer": "^6.0.3",
    "@types/node": "^12.0.4",
    "@types/ora": "^3.2.0",
    "@types/semver": "^6.0.0",
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "execa": "^1.0.0",
    "inquirer": "^6.3.1",
    "ora": "^3.4.0",
    "semver": "^6.1.1",
    "typescript": "^3.5.1"
  },
  "resolutions": {
    "@types/lodash": "4.14.119"
  }
}
